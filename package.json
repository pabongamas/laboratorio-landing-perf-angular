{
  "name": "landing-perf-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "http-server ./dist/landing-perf-angular -p 8080",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "analyze": "npm run build:stats && webpack-bundle-analyzer dist/landing-perf-angular/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lhci": "npm run build && lhci autorun --config=lighthouserc.js",
    "lhci:ssr": "npm run build:ssr && lhci autorun --config=lighthouserc-ssr.js",
    "lint": "ng lint",
    "images": "npm run images:resize && npm run images:compress",
    "images:resize": "node scripts/resize.mjs",
    "images:compress": "node scripts/compress.mjs",
    "dev:ssr": "ng run landing-perf-angular:serve-ssr",
    "serve:ssr": "node dist/landing-perf-angular/server/main.js",
    "build:ssr": "ng build && ng run landing-perf-angular:server",
    "prerender": "ng run landing-perf-angular:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.5",
    "@angular/common": "16.2.5",
    "@angular/compiler": "16.2.5",
    "@angular/core": "16.2.5",
    "@angular/forms": "16.2.5",
    "@angular/platform-browser": "16.2.5",
    "@angular/platform-browser-dynamic": "16.2.5",
    "@angular/router": "16.2.5",
    "@fortawesome/angular-fontawesome": "0.11.1",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@nguniversal/express-engine": "^16.2.0",
    "chart.js": "3.8.0",
    "date-fns": "^2.30.0",
    "express": "^4.15.2",
    "ng2-charts": "3.1.2",
    "rxjs": "7.5.0",
    "swiper": "8.3.0",
    "tslib": "2.3.0",
    "zone.js": "0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.2",
    "@angular-eslint/builder": "16.1.2",
    "@angular-eslint/eslint-plugin": "16.1.2",
    "@angular-eslint/eslint-plugin-template": "16.1.2",
    "@angular-eslint/schematics": "16.1.2",
    "@angular-eslint/template-parser": "16.1.2",
    "@angular/cli": "16.2.2",
    "@angular/compiler-cli": "16.2.5",
    "@lhci/cli": "0.9.0",
    "@nguniversal/builders": "^16.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "http-server": "14.1.1",
    "imagemin": "^8.0.1",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-webp": "^7.0.0",
    "jasmine-core": "4.1.0",
    "karma": "6.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.0.0",
    "karma-jasmine-html-reporter": "1.7.0",
    "sharp": "^0.30.7",
    "typescript": "4.9.5",
    "webpack-bundle-analyzer": "4.5.0"
  }
}